<h1>Show Project Details</h1>

<p id="notice"><%= notice %></p>

<div class='title'>
  Title: <%= @project.title %>
  Created: <%= @project.created_at.strftime('%m/%d/%Y %H:M') %>
</div>
<div class='project'>
  <%= @project.description %>
</div>
<div class='project'>
  <%= @project.url %>
</div>
<div class='project'>
  <%= @project.picture %>
</div>

<br />
<%= link_to 'Edit', edit_admin_project_path(@project) %> <br />
<%= link_to 'Back', admin_projects_path %>   <br />
<%= link_to 'Return to Home', root_path %>
